<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.zit.waeims.vo">
<class name="RoleVO" table="sys_role" >
	<id name="id">
		<generator class="uuid"></generator>
	</id>
	<property name="name" not-null="true"  length="32"></property>
	<property name="icon"  length="256"></property>
	<property name="state" not-null="true" type="integer" length="1"></property>
	<property name="remark"   length="256"></property>
</class>
</hibernate-mapping>
